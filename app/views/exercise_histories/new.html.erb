<%= form_for @exercise_history, url: {action: "create"} do |f| %>
  Name :<%= f.text_field :name %> <br/>
  Sets :<%= f.text_field :sets %> <br/>
  Reps :  <%= f.text_field :reps %> <br/>
  Weight <%= f.text_field :weight %> <br/>
  Tempo <%= f.text_field :tempo %> <br/>

  <%= f.submit "Create" %>
<% end %>
