<%= link_to "create new workout", new_user_workout_path  %><br/>
<% current_user.workouts.each do |workout| %>
  <%= link_to "#{workout.name}", edit_workout_path(current_user.id) %><br/>
  <%= link_to "Add A New Exercise To Workout",  new_workout_exercise_path(workout.id) %><br/>
  <% workout.exercises.each do |exercise| %>
    <%= link_to "#{exercise.name}", new_exercise_exercise_history_path(exercise.id) %>
  <% end %><br/><br/>
<% end %>

